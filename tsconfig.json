{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "removeComments": true,
    "jsx": "react-jsx",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "skipLibCheck": true,
    "lib": ["dom", "es6", "es7", "ES2020", "es2022.error"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true
  }
}
